    <%- include('elems/header.ejs', { isIndex: true }); %>

    <main>
        <div class="welcome-main-block">
            <div class="welcome-block">
                <div class="description-block">
                    <div class="welcome-text">
                        <%- locale.welcome_message %>
                    </div>
                    <div class="description-text" style="max-width: 400px;">
                        <%- locale.welcome_description %>
                    </div>
                </div>
            </div>

            <div class="welcome-buttons">
                <a id="open-in-browser" style="margin-top:10px; width: 250px; max-height: 50px; color: var(--primary-color);" class="button bold square" href="/signin">
                    <marquee direction="up" scrolldelay="300" behavior="scrolling">
                        <p>
                            <%- locale.welcome_web_button %>
                        </p>
                        
                        <p style="margin-top: 25px;">
                            <%- locale.welcome_web_button_more %>
                        </p>

                        <p style="margin-top: 25px;">
                            <%- locale.welcome_web_button_share %>
                        </p>
                    </marquee>

